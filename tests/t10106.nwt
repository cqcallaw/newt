# test basic using block with conflict between setup return type and result type

error_list {
	code:int,
	message:string,
	next:error_list?
}


result {
	value:int
	| errors:error_list
}

disposable {
	setup := (this:disposable?) -> double? {
		return 3.0
	},
	teardown := (this:disposable?) -> error_list? {
		return nil
	}
}

r := using @disposable as value -> result {
	return 1
}