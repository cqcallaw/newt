# test basic using block

error_list {
	code:int,
	message:string,
	next:error_list?
}


result {
	value:int
	| errors:error_list
}

disposable {
	not_setup := (this:disposable?) -> error_list? {
		return @error_list with { code = 3, message = "you dun goofed!" }
	},
	teardown := (this:disposable?) -> error_list? {
		return nil
	}
}

r := using @disposable as value -> result {
	return 1
}