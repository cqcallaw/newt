mutable circle {
	x: int,
	y: int,
	radius: int,
	w: int = 20
}

circles: circle[]
circles[9] = @circle
down: int = 0

p: int
i: int

for (i = 0; i < 10; i += 1)
{
    circles[i].radius = 10
    circles[i].x = i * 20 + 200
    circles[i].y = 400
}

for (p = 0; p < 20; p += 1) { 
    if (down == 9)
    {
        for (i = 0; i < 10; i += 1)
        {
            circles[i].y -= 20
        }
    }
    else
    {
        for (i = down; i >= 0; i -= 1)
        {
            circles[i].x = circles[i+1].x
            circles[i].y = circles[i+1].y - 20
        }
        down += 1
    }
}